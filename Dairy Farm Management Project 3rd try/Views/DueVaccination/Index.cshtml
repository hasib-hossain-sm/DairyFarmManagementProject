@model Dairy_Farm_Management_Project_3rd_try.Models.dashboardInfo
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<head>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Lato:wght@300;400;700;900&family=Mukta:wght@300;400;600;700;800&family=Noto+Sans:wght@400;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="css/style.css" type="text/css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.0.7/css/font-awesome.min.css">
    <!-- Code by Angela Delise
    https://codepen.io/angeladelise/full/LYREErG -->

</head>

<body class="newbody">
    

    <table class="newtable">
        <thead class="newthead">
            <tr class="newtr">
                <th class="newth">Diesease Name</th>
                <th class="newth">Vaccine No.</th>
                <th class="newth">Tag No.</th>

            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model.dueTobeVaccinatedlist.ToList())
            {

                <tr class="newtr">
                    <td class="newtd">@item.VaccineName</td>
                    <td class="newtd">@item.VaccineNo</td>
                    <td class="newtd">@item.TagNo</td>
                    
                </tr>

            }

        </tbody>
    </table>
</body>
